{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run open:browser\"",
    "open:browser": "node -e \"setTimeout(() => {}, 2000)\" && open-cli http://localhost:3000",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server.js",
    "test:pitch": "node tests/run-pitch-automation.js",
    "test:pitch:existing": "node tests/run-pitch-existing-browser.js",
    "test:pitch:case-studies": "node tests/run-pitch-existing-browser.js slides/case_studies.txt",
    "test:pitch:pnl": "node tests/run-pitch-existing-browser.js slides/pnl.txt",
    "test:pitch:angles": "node tests/run-pitch-existing-browser.js slides/angles.txt",
    "test:pitch:new": "node tests/run-pitch-new-presentation.js",
    "test:pitch:new:case-studies": "node tests/run-pitch-new-presentation.js slides/case_studies.txt",
    "test:pitch:new:pnl": "node tests/run-pitch-new-presentation.js slides/pnl.txt",
    "test:pitch:new:angles": "node tests/run-pitch-new-presentation.js slides/angles.txt",
    "test:pitch:parallel": "node tests/create-all-presentations-parallel.js",
    "test:pitch:combined": "node tests/create-combined-presentation.js",
    "test:pitch:playwright": "playwright test tests/pitch-automation.test.js --headed",
    "test:pitch:headless": "playwright test tests/pitch-automation.test.js"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "express": "^4.18.2",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "open-cli": "^8.0.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^7.2.4"
  }
}
